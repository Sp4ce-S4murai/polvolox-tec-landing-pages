---
---

<section class="py-16 bg-gray-800/50">
  <div class="max-w-7xl mx-auto px-4 sm:px-6 lg:px-8">
    <div class="text-center">
      <h2 class="text-2xl font-bold text-gray-300 mb-12">
        Aprovado por negócios que decolaram.
      </h2>
      
      <!-- Logos Grid -->
      <div class="grid grid-cols-2 md:grid-cols-5 gap-8 items-center justify-items-center opacity-60 hover:opacity-80 transition-opacity duration-300">
        
        <!-- Logo 1 - TechStart -->
        <div class="flex items-center justify-center h-16 w-32">
          <svg viewBox="0 0 120 40" class="h-8 text-gray-400 fill-current">
            <rect x="10" y="10" width="20" height="20" rx="4"/>
            <text x="40" y="25" font-family="Inter" font-weight="600" font-size="14">TechStart</text>
          </svg>
        </div>
        
        <!-- Logo 2 - InnovateCorp -->
        <div class="flex items-center justify-center h-16 w-32">
          <svg viewBox="0 0 140 40" class="h-8 text-gray-400 fill-current">
            <circle cx="20" cy="20" r="10"/>
            <text x="40" y="25" font-family="Inter" font-weight="600" font-size="12">InnovateCorp</text>
          </svg>
        </div>
        
        <!-- Logo 3 - GrowthLab -->
        <div class="flex items-center justify-center h-16 w-32">
          <svg viewBox="0 0 120 40" class="h-8 text-gray-400 fill-current">
            <polygon points="15,25 20,15 25,25"/>
            <text x="35" y="25" font-family="Inter" font-weight="600" font-size="14">GrowthLab</text>
          </svg>
        </div>
        
        <!-- Logo 4 - NextGen -->
        <div class="flex items-center justify-center h-16 w-32">
          <svg viewBox="0 0 110 40" class="h-8 text-gray-400 fill-current">
            <path d="M15 15 L25 15 L20 25 Z"/>
            <text x="35" y="25" font-family="Inter" font-weight="600" font-size="14">NextGen</text>
          </svg>
        </div>
        
        <!-- Logo 5 - ScaleUp -->
        <div class="flex items-center justify-center h-16 w-32">
          <svg viewBox="0 0 110 40" class="h-8 text-gray-400 fill-current">
            <rect x="12" y="12" width="16" height="16" rx="2" transform="rotate(45 20 20)"/>
            <text x="35" y="25" font-family="Inter" font-weight="600" font-size="14">ScaleUp</text>
          </svg>
        </div>
        
      </div>
      
      <!-- Stats -->
      <div class="mt-16 grid grid-cols-1 md:grid-cols-3 gap-8">
        <div class="text-center">
          <div class="text-3xl font-extrabold text-white mb-2">150+</div>
          <div class="text-gray-400">Páginas Entregues</div>
        </div>
        <div class="text-center">
          <div class="text-3xl font-extrabold text-white mb-2">98%</div>
          <div class="text-gray-400">Taxa de Satisfação</div>
        </div>
        <div class="text-center">
          <div class="text-3xl font-extrabold text-white mb-2">48h</div>
          <div class="text-gray-400">Tempo Médio de Entrega</div>
        </div>
      </div>
    </div>
  </div>
</section>